{
  "compileOnSave": false,
  "compilerOptions": {
    "rootDir": ".",
    "sourceMap": true,
    "declaration": false,
    "moduleResolution": "node",
    "esModuleInterop": true,
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "importHelpers": true,
    "target": "es2019",
    "module": "esnext",
    "lib": ["es2019", "dom"],
    "skipLibCheck": true,
    "skipDefaultLibCheck": true,
    "jsx": "react-jsx",
    "baseUrl": ".",
    "paths": {
      "@apps/artifacts/graphql/blocks": ["libs/artifacts/src/graphql/blocks.tsx"],
      "@apps/artifacts/graphql/feeders": ["libs/artifacts/src/graphql/feeders.tsx"],
      "@apps/artifacts/graphql/protocol": ["libs/artifacts/src/graphql/protocol.tsx"],
      "@apps/artifacts/graphql/shared": ["libs/artifacts/src/graphql/shared.tsx"],
      "@apps/artifacts/graphql/staking-rewards": ["libs/artifacts/src/graphql/staking-rewards.tsx"],
      "@apps/artifacts/typechain": ["libs/artifacts/src/typechain/index.ts"],
      "@apps/base": ["libs/base/src/index.ts"],
      "@apps/base/theme": ["libs/base/src/lib/theme.ts"],
      "@apps/base/context/account": ["libs/base/src/lib/context/AccountProvider.tsx"],
      "@apps/base/context/apollo": ["libs/base/src/lib/context/ApolloProvider.tsx"],
      "@apps/base/context/app": ["libs/base/src/lib/context/AppProvider.tsx"],
      "@apps/base/context/block": ["libs/base/src/lib/context/BlockProvider.tsx"],
      "@apps/base/context/data": ["libs/base/src/lib/context/DataProvider/index.ts"],
      "@apps/base/context/masset": ["libs/base/src/lib/context/MassetProvider.tsx"],
      "@apps/base/context/network": ["libs/base/src/lib/context/NetworkProvider.tsx"],
      "@apps/base/context/notifications": ["libs/base/src/lib/context/NotificationsProvider.tsx"],
      "@apps/base/context/prices": ["libs/base/src/lib/context/PricesProvider.tsx"],
      "@apps/base/context/theme": ["libs/base/src/lib/context/ThemeProvider.tsx"],
      "@apps/base/context/tokens": ["libs/base/src/lib/context/TokensProvider.tsx"],
      "@apps/base/context/transactions": ["libs/base/src/lib/context/TransactionsProvider.tsx"],
      "@apps/bigdecimal": ["libs/bigdecimal/src/index.ts"],
      "@apps/components": ["libs/components/src/index.ts"],
      "@apps/components/core": ["libs/components/src/lib/core/index.ts"],
      "@apps/components/forms": ["libs/components/src/lib/forms/index.ts"],
      "@apps/components/icons": ["libs/components/src/lib/icons/index.ts"],
      "@apps/components/icons/*": ["libs/components/src/lib/icons/*"],
      "@apps/formatters": ["libs/formatters/src/index.ts"],
      "@apps/hooks": ["libs/hooks/src/index.ts"],
      "@apps/quick-maths": ["libs/quick-maths/src/index.ts"],
      "@apps/react-utils": ["libs/react-utils/src/index.ts"],
      "@apps/transaction-manifest": ["libs/transaction-manifest/src/index.ts"],
      "@apps/types": ["libs/types/src/index.ts"]
    }
  },
  "include": ["./*.d.ts"],
  "exclude": ["node_modules", "tmp"]
}
