        "test": {
          "executor": "@nrwl/jest:jest",
          "outputs": [
            "coverage/libs/artifacts"
          ],
          "options": {
            "jestConfig": "libs/artifacts/jest.config.js",
            "passWithNoTests": true
          }
        }
      }
    },
